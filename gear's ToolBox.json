{
	"request": [
		{
			"enable": true,
			"name": "Google APIs",
			"ruleType": "redirect",
			"matchType": "regexp",
			"pattern": "^http(s?)://ajax\\.googleapis\\.com/(.*)",
			"exclude": "",
			"isFunction": false,
			"action": "redirect",
			"to": "https://gapis.geekzu.org/ajax/$2",
			"group": "gear's ToolBox"
		},
		{
			"enable": true,
			"name": "reCaptcha",
			"ruleType": "redirect",
			"matchType": "regexp",
			"pattern": "^http(s?)://(?:www\\.|recaptcha\\.|)google\\.com/recaptcha/(.*)",
			"exclude": "",
			"isFunction": false,
			"action": "redirect",
			"to": "https://recaptcha.net/recaptcha/$2",
			"group": "gear's ToolBox"
		},
		{
			"enable": true,
			"name": "字体库",
			"ruleType": "redirect",
			"matchType": "domain",
			"pattern": "fonts.googleapis.com",
			"exclude": "",
			"group": "gear's ToolBox",
			"isFunction": false,
			"action": "redirect",
			"to": "fonts.geekzu.org"
		},
		{
			"enable": true,
			"name": "字体文件",
			"ruleType": "redirect",
			"matchType": "regexp",
			"pattern": "^http(s?)://fonts\\.gstatic\\.com/",
			"exclude": "",
			"group": "gear's ToolBox",
			"isFunction": false,
			"action": "redirect",
			"to": "https://gapis.geekzu.org/g-fonts/"
		},
		{
			"enable": true,
			"name": "google API",
			"ruleType": "cancel",
			"matchType": "domain",
			"pattern": "apis.google.com",
			"exclude": "",
			"group": "gear's ToolBox",
			"isFunction": false,
			"action": "cancel"
		},
		{
			"enable": true,
			"name": "google翻译",
			"ruleType": "redirect",
			"matchType": "domain",
			"pattern": "translate.google.com",
			"exclude": "",
			"group": "gear's ToolBox",
			"isFunction": false,
			"action": "redirect",
			"to": "translate.google.cn"
		},
		{
			"enable": true,
			"name": "MCBBS个人空间页跳转(a)",
			"ruleType": "redirect",
			"matchType": "regexp",
			"pattern": "^https?://www\\.mcbbs\\.net/home\\.php\\?mod=space\\&uid=(\\d+)$",
			"exclude": "",
			"group": "gear's ToolBox",
			"isFunction": false,
			"action": "redirect",
			"to": "https://www.mcbbs.net/home.php?mod=space&uid=$1&view=admin"
		},
		{
			"enable": true,
			"name": "Adfoc广告跳过",
			"ruleType": "redirect",
			"matchType": "regexp",
			"pattern": "https?://adfoc\\.us/serve/sitelinks/\\?id=\\d+\\&url=(\\w+)",
			"exclude": "",
			"group": "gear's ToolBox",
			"isFunction": false,
			"action": "redirect",
			"to": "http://adfoc.us/serve/sitelinks/?id=545355&url=$1"
		},
		{
			"enable": true,
			"name": "AdFly跳过广告",
			"ruleType": "redirect",
			"matchType": "regexp",
			"pattern": "https?://www\\da\\.\\w+?\\.pro/pushredirect/\\?.*&dest=(.*)",
			"exclude": "",
			"group": "gear's ToolBox",
			"isFunction": false,
			"code": "val = decodeURIComponent(val)",
			"action": "redirect",
			"to": "$1"
		},
		{
			"enable": true,
			"name": "MCBBS外链跳转",
			"ruleType": "redirect",
			"matchType": "regexp",
			"pattern": "^https?://www\\.mcbbs\\.net/plugin\\.php\\?.+target=(.*)",
			"exclude": "",
			"group": "gear's ToolBox",
			"isFunction": false,
			"action": "redirect",
			"to": "$1"
		},
		{
			"enable": true,
			"name": "MCBBS PHP转HTML",
			"ruleType": "redirect",
			"matchType": "regexp",
			"pattern": "^https?://www\\.mcbbs\\.net/forum\\.php\\?mod=viewthread.*?\\&tid=(\\d+).*?\\&page=(\\d+)([^\\#]*)",
			"exclude": "",
			"group": "gear's ToolBox",
			"isFunction": false,
			"action": "redirect",
			"to": "https://www.mcbbs.net/thread-$1-$2-1.html"
		},
		{
			"enable": true,
			"name": "MCBBS PHP转HTML(无page参数)",
			"ruleType": "redirect",
			"matchType": "regexp",
			"pattern": "^https?://www\\.mcbbs\\.net/forum\\.php\\?mod=viewthread.*?\\&tid=(\\d*).*?([^\\#]*)",
			"exclude": "^https?://www\\.mcbbs\\.net/forum\\.php\\?mod=viewthread.*?\\&tid=(\\d+).*?\\&page=(\\d+)([^\\#]*)",
			"group": "gear's ToolBox",
			"isFunction": false,
			"action": "redirect",
			"to": "https://www.mcbbs.net/thread-$1-1-1.html",
			"_exclude": {}
		},
		{
			"enable": true,
			"name": "知乎外链自动跳转",
			"ruleType": "redirect",
			"matchType": "regexp",
			"pattern": "^https?://link\\.zhihu\\.com/\\?target=(.*)",
			"exclude": "",
			"group": "gear's ToolBox",
			"isFunction": false,
			"action": "redirect",
			"to": "$1"
		},
		{
			"enable": true,
			"name": "百度搜索多余参数清除",
			"ruleType": "redirect",
			"matchType": "regexp",
			"pattern": "^https?://www\\.baidu\\.com/s\\?.*?wd=([^&]*).*",
			"exclude": "",
			"group": "gear's ToolBox",
			"isFunction": false,
			"action": "redirect",
			"to": "https://www.baidu.com/s?wd=$1"
		}
	],
	"sendHeader": [
		{
			"enable": true,
			"name": "firefox商店UA修复",
			"ruleType": "modifySendHeader",
			"matchType": "domain",
			"pattern": "addons.mozilla.org",
			"exclude": "",
			"group": "gear's ToolBox",
			"isFunction": false,
			"action": {
				"name": "user-agent",
				"value": "Mozilla/5.0 (Windows NT 10.0; WOW64; rv:56.0) Gecko/20100101 Firefox/56.0"
			}
		},
		{
			"enable": true,
			"name": "百度网盘下载UA修复",
			"ruleType": "modifySendHeader",
			"matchType": "regexp",
			"pattern": "^https?://\\w+\\.baidupcs\\.com/file/.+",
			"exclude": "",
			"group": "gear's ToolBox",
			"isFunction": false,
			"action": {
				"name": "user-agent",
				"value": "LogStatistic"
			}
		}
	],
	"receiveHeader": [],
	"receiveBody": []
}